;
; BIND data file for local loopback interface
;
$TTL 2d
$ORIGIN chubut.dc.fi.uba.ar.
@	IN	SOA	dnsroot.chubut.dc.fi.uba.ar. root.fi.uba.ar. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; definicion del dns root
		IN	NS	dnsroot.chubut.dc.fi.uba.ar.
dnsroot		IN	A	192.168.0.6

$ORIGIN riocolorado.chubut.dc.fi.uba.ar.
; Zona rio colorado
@		IN	NS	dns1.riocolorado.chubut.dc.fi.uba.ar.
dns1		IN	A	192.168.0.7

$ORIGIN bariloche.chubut.dc.fi.uba.ar.
; Zona resto
@		IN	NS	dns2.bariloche.chubut.dc.fi.uba.ar.
dns2		IN	A	192.168.0.8

$ORIGIN trelew.chubut.dc.fi.uba.ar.
; Zona resto
@		IN	NS	dns2.trelew.chubut.dc.fi.uba.ar.
dns2		IN	A	192.168.0.8

$ORIGIN resto.chubut.dc.fi.uba.ar.
; Zona resto
@		IN	NS	dns2.resto.chubut.dc.fi.uba.ar.
dns2		IN	A	192.168.0.8
